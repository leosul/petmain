{
  "name": "petmain",
  "version": "1.0.0",
  "description": "Project to pets",
  "main": "index.js",
  "scripts": {
    "prestart": "npm run build",
    "start": "node index.js",
    "build": "cd client && npm run build",
    "client": "cd client && npm run start",
    "server": "nodemon index.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "lint": "standard",
    "pretest": "npm run lint",
    "test": "nyc --reporter=html mocha --recursive --timeout=30000"
  },
  "author": "Leonardo Jacques da Silva",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fb": "^2.0.0",
    "google-auth-library": "^5.9.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose-paginate-v2": "^1.3.6"
  },
  "devDependencies": {
    "mocha": "^7.0.1",
    "mongoose": "^5.9.1",
    "nodemon": "^2.0.2"
  }
}
