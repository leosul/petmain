        "react": "^16.6.3",
        "react-dom": "^16.6.3",
        "react-facebook-login": "^4.1.1",
        "react-google-login": "^5.0.4",
        "react-infinite-scroller": "^1.2.4",
        "react-redux": "^6.0.0",
        "react-router-dom": "^4.3.1",
        "react-scripts": "^3.4.1",
